{
    "name": "mobihouse/laravel-pennant-cookie",
    "description": "A cookie driver for Laravel Pennant",
    "version": "1.0.0-beta2",
    "type": "library",
    "require-dev": {
        "orchestra/testbench": "^9.2",
        "pestphp/pest": "^2.34",
        "orchestra/pest-plugin-testbench": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mobihouse\\LaravelPennantCookie\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Bas Tuijnman"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "laravel/pennant": "^1.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mobihouse\\LaravelPennantCookie\\LaravelPennantCookieServiceProvider"
            ]
        }
    }
}
